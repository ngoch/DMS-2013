@model TestAssessmentQuestionViewModel
@using (Html.BeginForm())
{
    @Model.QuestionText
    <ol>
        @for (int i = 0; i < Model.AllFactors.Count; i++)
        {
            var symtom = Model.AllFactors[i];
            var @checked = Model.ChosenFactors.Any(chosen => chosen == symtom.FactorId);
            <li>
                <label>@symtom.FactorName
                    <input id="ChosenFactors" name="ChosenFactors" type="checkbox" value="@symtom.FactorId"
                    @if (@checked)
                    {
              <text>checked="checked"</text>          
                    }></label>
            </li>
        }
    </ol>    
    <input type="submit" value="next" />
}
